<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".questions.Question5Fragment">

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Спорт:"
        android:textColor="#66B9F9"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="468dp"
        android:text="Физическая активность:"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggle12"
        app:layout_constraintVertical_bias="0.172" />

    <ToggleButton
        android:id="@+id/toggle13"
        android:layout_width="112dp"
        android:layout_height="102dp"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="12dp"
        android:text="ToggleButton"
        android:textOff="пользуюсь меньше 4-х раз в день"
        android:textOn="пользуюсь меньше 4-х раз в день"
        app:layout_constraintStart_toEndOf="@+id/toggle12"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <ToggleButton
        android:id="@+id/toggle11"
        android:layout_width="112dp"
        android:layout_height="102dp"
        android:layout_marginTop="16dp"
        android:layout_marginRight="12dp"
        android:text="ToggleButton"
        android:textOff="пользуюсь меньше 2-х раз в неделю"
        android:textOn="пользуюсь меньше 2-х раз в неделю"
        app:layout_constraintEnd_toStartOf="@+id/toggle12"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <ToggleButton
        android:id="@+id/toggle12"
        android:layout_width="112dp"
        android:layout_height="102dp"
        android:layout_marginTop="16dp"
        android:text="ToggleButton"
        android:textOff="пользуюсь меньше 10-ти раз в неделю"
        android:textOn="пользуюсь меньше 10-ти раз в неделю"
        app:layout_constraintEnd_toEndOf="@+id/textView37"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/textView37"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <ToggleButton
        android:id="@+id/toggle22"
        android:layout_width="112dp"
        android:layout_height="102dp"
        android:layout_marginTop="20dp"
        android:text="ToggleButton"
        android:textOff="пользуюсь меньше 5-ти раз в месяц"
        android:textOn="меньше 5-ти раза в месяц"
        app:layout_constraintEnd_toEndOf="@+id/textView38"
        app:layout_constraintTop_toBottomOf="@+id/textView38" />

    <ToggleButton
        android:id="@+id/toggle21"
        android:layout_width="112dp"
        android:layout_height="102dp"
        android:layout_marginTop="20dp"
        android:text="ToggleButton"
        android:textOff="пользуюсь меньше 1-го раз в месяц"
        android:textOn="пользуюсь меньше 1-го раз в месяц"
        app:layout_constraintStart_toStartOf="@+id/textView38"
        app:layout_constraintTop_toBottomOf="@+id/textView38" />

</androidx.constraintlayout.widget.ConstraintLayout>