<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".questions.Question5Fragment">

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Спорт:"
        android:textColor="#66B9F9"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Физическая активность:"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/textView37"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="@+id/textView37"
        app:layout_constraintTop_toBottomOf="@+id/toggle12" />

    <ToggleButton
        android:id="@+id/toggle13"
        android:layout_width="150dp"
        android:layout_height="90dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="16dp"
        android:text="ToggleButton"
        android:textOff="не занимаюсь спортом"
        android:textOn="не занимаюсь спортом"
        app:layout_constraintStart_toEndOf="@+id/toggle12"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <ToggleButton
        android:id="@+id/toggle11"
        android:layout_width="150dp"
        android:layout_height="90dp"
        android:layout_marginTop="16dp"
        android:layout_marginRight="12dp"
        android:text="ToggleButton"
        android:textOff="регулярно(несколько раз в неделю)"
        android:textOn="регулярно(несколько раз в неделю)"
        app:layout_constraintEnd_toStartOf="@+id/toggle12"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <ToggleButton
        android:id="@+id/toggle12"
        android:layout_width="150dp"
        android:layout_height="90dp"
        android:layout_marginTop="16dp"
        android:text="ToggleButton"
        android:textOff="редко(один раз в неделю или реже)"
        android:textOn="редко(один раз в неделю или реже)"
        app:layout_constraintEnd_toEndOf="@+id/textView37"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/textView37"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <ToggleButton
        android:id="@+id/toggle22"
        android:layout_width="150dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:text="ToggleButton"
        android:textOff="сидячий образ жизни"
        android:textOn="сидячий образ жизни"
        app:layout_constraintEnd_toEndOf="@+id/toggle13"
        app:layout_constraintHorizontal_bias="0.764"
        app:layout_constraintStart_toStartOf="@+id/textView38"
        app:layout_constraintTop_toBottomOf="@+id/textView38" />

    <ToggleButton
        android:id="@+id/toggle21"
        android:layout_width="150dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:text="ToggleButton"
        android:textOff="активный образ жизни"
        android:textOn="активный образ жизни"
        app:layout_constraintEnd_toEndOf="@+id/textView38"
        app:layout_constraintHorizontal_bias="0.338"
        app:layout_constraintStart_toStartOf="@+id/toggle11"
        app:layout_constraintTop_toBottomOf="@+id/textView38" />

</androidx.constraintlayout.widget.ConstraintLayout>